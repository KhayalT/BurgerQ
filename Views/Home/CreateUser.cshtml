<body style="background-image:url(/burgerq/assets/images/g3.jpg)">
    <div class="mid-class">
        <div class="art-right-w3ls">
            <h2>
                <a href="/Home/Index/">
                    Geri Dön
                </a> və ya Qeydiyyatdan keç.
            </h2>
            <form action="#" method="post">
                <div class="main">
                    <div class="form-left-to-w3l">
                        <input type="text" name="Name" placeholder="Ad" required="">
                    </div>
                    <div class="form-left-to-w3l ">
                        <input type="text" name="LasName" placeholder="Soyad" required="">
                        <div class="clear"></div>
                    </div>
                    <div class="form-left-to-w3l ">
                        <input type="email" name="Email" placeholder="Email" required="">
                        <div class="clear"></div>
                    </div>
                    <div class="form-left-to-w3l ">

                        @Html.TextBoxFor(model => model.telephone, new { @class = "textbox", placeholder="Telefon Nömrəniz" })

                        <div class="clear"></div>
                    </div>
                    <div class="form-left-to-w3l ">
                        <input type="password" name="password" placeholder="Parol" required="">
                        <div class="clear"></div>
                    </div>
                </div>
                <div class="clear"></div>
                <div class="btnn">
                    <button type="submit">Qeyydiyyatdan Keç</button>
                </div>
            </form>
        </div>
    </div>
